<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qa.edu.qu.cse.cmps497.servicesremote">

    <permission android:name="qa.edu.qu.cse.cmps497.servicesremote.PERMISSION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".MessengerRemoteService"
            android:enabled="true"
            android:exported="true"
            android:permission="qa.edu.qu.cse.cmps497.servicesremote.PERMISSION">
            <intent-filter>
                <action android:name="qa.edu.qu.cse.cmps497.servicesremote.messenger_service_action" />
            </intent-filter>
        </service>
        <service
            android:name=".AIDLRemoteService"
            android:enabled="true"
            android:exported="true"
            android:permission="qa.edu.qu.cse.cmps497.servicesremote.PERMISSION">
            <intent-filter>
                <action android:name="qa.edu.qu.cse.cmps497.servicesremote.aidl_service_action" />
            </intent-filter>
        </service>
    </application>

</manifest>
